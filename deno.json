{
    "tasks": {
        "start": "cd $INIT_CWD && deno run --allow-read --allow-env ./main.ts",
        "run": "deno run --allow-read --allow-run bin/run.ts"
    },
    "imports": {
        "@util": "./util/util.ts",
        "@types/node": "npm:@types/node@^22.10.1"
    },
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUncheckedIndexedAccess": true
    }
}
